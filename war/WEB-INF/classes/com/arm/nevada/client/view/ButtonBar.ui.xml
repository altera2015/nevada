<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<ui:with field="res"
		type='com.arm.nevada.client.view.design.ResourceBundle' />
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:PushButton ui:field="stepButton" title="Step">
					<g:upFace image="{res.step}" />
				</g:PushButton>
				<g:PushButton ui:field="runButton" title="Run">
					<g:upFace image="{res.runToBreakpoint}" />
				</g:PushButton>
				<g:PushButton ui:field="resetButton" title="Reset">
					<g:upFace image="{res.reset}" />
				</g:PushButton>
				<g:PushButton ui:field="viewEditButton" title="Toggle edit/view">
					<g:upFace image="{res.editView}" />
				</g:PushButton>
				<g:PushButton ui:field="loadSaveButton" title="Load and Save">
					<g:upFace image="{res.loadSave}" />
				</g:PushButton>
				<g:PushButton ui:field="helpButton" title="Help">
					<g:upFace image="{res.help}" />
				</g:PushButton>
				<g:PushButton height="32px" ui:field="randomizeButton" title="Randomize"
					text="Random" width="50px">
				</g:PushButton>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 