<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.arm.nevada.client.view">

	<ui:style>
		.reg {
			width: 100%;
		}
	</ui:style>
	<ui:with field="res"
		type="com.arm.nevada.client.view.design.ResourceBundle" />
	<g:HorizontalPanel width="100%" stylePrimaryName="{res.style.armRegister}"
		ui:field="container">
		<g:HTMLPanel>
			<table>
				<tr>
					<td style="width: 100%">
						<c:BasicRegister ui:field="register"
							addStyleNames="{style.reg}" />
					</td>
					<td>
						<g:Label addStyleNames="{res.style.indexLabel}" ui:field="label">Rx</g:Label>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>

</ui:UiBinder> 