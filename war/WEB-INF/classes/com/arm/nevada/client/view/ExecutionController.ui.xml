<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<ui:with field="res"
		type='com.arm.nevada.client.view.design.ResourceBundle' />
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:PushButton ui:field="toNextBreakpointButton" title="Run to the next breakpoint">
				<g:upFace image="{res.runToBreakpoint}" />
			</g:PushButton>
			<g:PushButton ui:field="stepButton" title="Step">
				<g:upFace image="{res.step}" />
			</g:PushButton>
			<g:PushButton ui:field="executeToEndButton" title="Run to the end">
				<g:upFace image="{res.runToEnd}" />
			</g:PushButton>
			<g:PushButton ui:field="resetExecutionButton" title="Reset">
				<g:upFace image="{res.reset}" />
			</g:PushButton>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 