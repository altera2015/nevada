<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel width="100%">
		<g:MenuBar width="100%" vertical="false">
			<g:MenuItem text="Display type" ui:field="rootItem">
				<g:MenuBar vertical="false">
					<g:MenuItem text=".8">
						<g:MenuBar vertical="false">
							<g:MenuItem text="Signed">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="sd8" text="Decimal" />
									<g:MenuItem ui:field="sh8" text="Hexa" />									
								</g:MenuBar>
							</g:MenuItem>
							<g:MenuItem text="Unsigned">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="ud8" text="Decimal" />
									<g:MenuItem ui:field="uh8" text="Hexa" />									
								</g:MenuBar>
							</g:MenuItem>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text=".16">
						<g:MenuBar vertical="false">
							<g:MenuItem text="Signed">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="sd16" text="Decimal" />
									<g:MenuItem ui:field="sh16" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>
							<g:MenuItem text="Unsigned">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="ud16" text="Decimal" />
									<g:MenuItem ui:field="uh16" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text=".32">
						<g:MenuBar vertical="false">
							<g:MenuItem text="Signed">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="sd32" text="Decimal" />
									<g:MenuItem ui:field="sh32" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>
							<g:MenuItem text="Unsigned">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="ud32" text="Decimal" />
									<g:MenuItem ui:field="uh32" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>

						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text=".64" ui:field="doubleWideMeuItem">
						<g:MenuBar vertical="false">
							<g:MenuItem text="Signed">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="sd64" text="Decimal" />
									<g:MenuItem ui:field="sh64" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>
							<g:MenuItem  visible="false" text="Unsigned">
								<g:MenuBar vertical="false">
									<g:MenuItem ui:field="ud64" text="Decimal" />
									<g:MenuItem ui:field="uh64" text="Hexa" />
								</g:MenuBar>
							</g:MenuItem>
						</g:MenuBar>
					</g:MenuItem>
					<g:MenuItem text="Float">
						<g:MenuBar vertical="false">
							<g:MenuItem visible="false" ui:field="f16" text="Float 16" />
							<g:MenuItem ui:field="f32" text="Float 32" />
						</g:MenuBar>
					</g:MenuItem>
				</g:MenuBar>
			</g:MenuItem>
		</g:MenuBar>


	</g:HTMLPanel>
</ui:UiBinder> 