<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="100%">
			<g:cell>
				<g:Label>URL</g:Label>
			</g:cell>			
		</g:HorizontalPanel>
		<g:TextArea ui:field="URLTextArea" width="300px" height="50px" />
		<g:HorizontalPanel width="100%">
			<g:cell>
				<g:Label>Machine</g:Label>
			</g:cell>
			<g:cell horizontalAlignment="right">
				<g:Button ui:field="machineLoadButton">Load</g:Button>
			</g:cell>
		</g:HorizontalPanel>
		<g:TextArea ui:field="machineTextArea" width="300px" height="50px" />
		<g:HorizontalPanel width="100%">
			<g:cell>
				<g:Label>Editor</g:Label>
			</g:cell>
			<g:cell horizontalAlignment="right">
				<g:Button ui:field="editorLoadButton">Load</g:Button>
			</g:cell>
		</g:HorizontalPanel>
		<g:TextArea ui:field="editorTextArea" width="300px" height="50px" />
	</g:HTMLPanel>
</ui:UiBinder> 