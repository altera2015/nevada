<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>

	<g:HTMLPanel>
		<g:DialogBox autoHide="true" modal="true" glassEnabled="true"
			animationEnabled="true" ui:field="dialogBox">
			<g:HTMLPanel>
				<table>
					<tr>
						<td>
							Size in words:
						</td>
						<td>
							<g:TextBox name="Memory size in words" ui:field="sizeTextBox">0</g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							Offset in words:
						</td>
						<td>
							<g:TextBox name="Memory offset in words" ui:field="offsetTextBox">0</g:TextBox>
						</td>
					</tr>
					<tr>
						<td colspan="2">
							<g:Button ui:field="saveButton">Save</g:Button>
							<g:Button ui:field="cancelButton">Cancel</g:Button>
						</td>
					</tr>


				</table>



			</g:HTMLPanel>
		</g:DialogBox>
	</g:HTMLPanel>

</ui:UiBinder> 